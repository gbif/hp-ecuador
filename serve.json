{
  "rewrites": [
    {
      "source": "/institution/*-*",
      "destination": "/institution/_key_.html"
    },
    {
      "source": "/institution/*-*/*",
      "destination": "/institution/_key_.html"
    },
    {
      "source": "/collection/*-*",
      "destination": "/collection/_key_.html"
    },
    {
      "source": "/collection/*-*/*",
      "destination": "/collection/_key_.html"
    }
  ]
}